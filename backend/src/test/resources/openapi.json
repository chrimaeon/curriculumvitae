  "components" : {
    "schemas" : {
      "Address" : {
        "required" : [ "city", "postalCode", "street" ],
        "type" : "object",
        "properties" : {
          "city" : {
            "type" : "string"
          },
          "postalCode" : {
            "type" : "string"
          },
          "street" : {
            "type" : "string"
          }
        }
      },
      "Profile" : {
        "required" : [ "address", "email", "intro", "name", "phone", "profileImagePath" ],
        "type" : "object",
        "properties" : {
          "address" : {
            "$ref" : "#/components/schemas/Address"
          },
          "email" : {
            "type" : "string"
          },
          "intro" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          },
          "name" : {
            "type" : "string"
          },
          "phone" : {
            "type" : "string"
          },
          "profileImagePath" : {
            "type" : "string"
          }
        }
      },
      "Employment" : {
        "required" : [ "city", "description", "employer", "endDate", "id", "jobTitle", "startDate" ],
        "type" : "object",
        "properties" : {
          "city" : {
            "type" : "string"
          },
          "description" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          },
          "employer" : {
            "type" : "string"
          },
          "endDate" : {
            "type" : "string",
            "format" : "date",
            "nullable" : true
          },
          "id" : {
            "type" : "integer",
            "format" : "int32"
          },
          "jobTitle" : {
            "type" : "string"
          },
          "startDate" : {
            "type" : "string",
            "format" : "date"
          }
        }
      },
      "Skill" : {
        "required" : [ "level", "name" ],
        "type" : "object",
        "properties" : {
          "level" : {
            "type" : "integer",
            "format" : "int32"
          },
          "name" : {
            "type" : "string"
          }
        }
      },
      "OssProject" : {
        "required" : [ "archived", "description", "fork", "name", "private", "pushedAt", "stars", "topics", "url" ],
        "type" : "object",
        "properties" : {
          "archived" : {
            "type" : "boolean"
          },
          "description" : {
            "type" : "string"
          },
          "fork" : {
            "type" : "boolean"
          },
          "name" : {
            "type" : "string"
          },
          "private" : {
            "type" : "boolean"
          },
          "pushedAt" : {
            "type" : "string",
            "format" : "date-time",
            "nullable" : true
          },
          "stars" : {
            "type" : "integer",
            "format" : "int32"
          },
          "topics" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          },
          "url" : {
            "type" : "string"
          }
        }
      }
    },
    "examples" : { }
  }
